# This file is being maintained by Puppet.
# DO NOT EDIT
[domain_realm]
 <%= @computer_object_name_real %> = <%= @realm %>

[libdefaults]
 default_realm = <%= @realm %>
 default_tgs_enctypes = arcfour-hmac-md5
 default_tkt_enctypes = arcfour-hmac-md5
 default_etypes_des = des-cbc-crc
 default_etypes = arcfour-hmac-md5
 renew_lifetime = 604800

 ticket_lifetime = 36000
 default_keytab_name = /etc/opt/quest/vas/host.keytab

[libvas]
<% if @sitenameoverride -%>
 site-name-override = <%= @sitenameoverride %>
<% end -%>

[pam_vas]

[vasypd]
 search-base = <%= @nis_search_base_real %>
 split-groups = true
 update-interval = 1800
 domainname-override = <%= @my_nisdomainname %>

[vasd]
<% if @upm_search_path -%>
 upm-search-path = <%= @upm_search_path %>
<% end -%>
 workstation-mode = <%= @workstation_mode_real %>
<% if @vas_user_override_path %>
 user-override-file = <%= @vas_user_override_path %>
<% end -%>
<% if @vas_group_override_path %>
 group-override-file = <%= @vas_group_override_path %>
<% end -%>

[nss_vas]
 group-update-mode = none
 root-update-mode = none

[vas_auth]
<% if @vas_users_allow_path -%>
 users-allow-file = <%= @vas_users_allow_path %>
<% end -%>
